{% extends "partials/base.twig" %}

{% block config %}
  {{ parent() }}

  {% if ENV == 'development' %}
    {% set site = site|merge(
      {fgi: 68}
    ) %}

    {% set page = page|merge(
      {
        classes: 'is-home',
        type: 'index'
      }
    ) %}
  {% endif %}
{% endblock %}

{% block head %}
  {{ parent() }}
  <link rel="stylesheet" href="{{ site.assets_prefix }}/assets/css/home.css?{{ "now"|date('U') }}">
{% endblock %}

{# MAIN CONTENT ------------------------------------------------------------- #}
{% block content %}

